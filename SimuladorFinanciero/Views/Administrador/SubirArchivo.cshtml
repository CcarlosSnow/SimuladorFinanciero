<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2>Cargar Archivo Base</h2>
            <br />
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-primary" data-collapsed="0">
                        <div class="panel-heading">
                            <div class="panel-title">
                                Subir Excel
                            </div>
                            <div class="panel-options">
                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                            </div>
                        </div>
                        <script>
                            $(document).ready(function () {
                                $('#btnSubmit').click(function () {
                                    debugger;
                                    var formData = new FormData();
                                    var file = $('#file').get(0).files[0];
                                    formData.append('file', file);
                                    $.ajax({
                                        type: "POST",
                                        url: '@Url.Action("Subir", "Administrador")',
                                        data: formData,
                                        dataType: 'json',
                                        contentType: false,
                                        processData: false,
                                        success: function (response) {
                                            alert(response);
                                        },
                                        error: function (error) {
                                            alert(error);
                                        }
                                    });
                                });
                            });
                        </script>
                        <div class="panel-body">
                            @using (Ajax.BeginForm("Subir", "Administrador", new AjaxOptions()
                            {
                                HttpMethod = "POST",
                                InsertionMode = InsertionMode.Replace
                            }, new { enctype = "multipart/form-data" }))
                            {
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Archivo Excel</label>
                                    <div class="col-sm-5">
                                        <input type="file" name="file" id="file" class="form-control file2 inline btn btn-primary" data-label="<i class='fa fa-file-excel-o' aria-hidden='true'></i> Buscar" />
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-offset-3 col-sm-5">
                                        <button type="button" class="btn btn-blue" id="btnSubmit">Subir Archivo</button>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
            <footer class="main">
                &copy; 2016 <strong>Administrador - Simulador Financiero</strong>
            </footer>
        </div>
    </div>
</div>
