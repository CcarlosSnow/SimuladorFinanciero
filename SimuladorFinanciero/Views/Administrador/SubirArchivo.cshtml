<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2>Cargar Archivo Base</h2>
            <br />
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-primary" data-collapsed="0">
                        <div class="panel-heading">
                            <div class="panel-title">
                                Subir Excel
                            </div>
                            <div class="panel-options">
                                <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                            </div>
                        </div>
                        <div class="panel-body">
                            @using (Ajax.BeginForm("Subir", "Administrador", new AjaxOptions()
                            {
                                HttpMethod = "POST",
                                InsertionMode = InsertionMode.Replace
                            }, new
                            {
                                id = "form-upload",
                                role = "form",
                                enctype = "multipart/form-data",
                                @class = "form-horizontal form-groups-bordered"
                            }))
                            {
                                <div class="form-group">
                                    <label class="col-sm-3 control-label">Subir archivo excel</label>
                                    <div class="col-sm-5">
                                        <div class="input-group">
                                            <input type="text" class="custom-input-02 form-control" readonly="" />
                                            <span class="input-group-btn">
                                                <span class="btn btn-primary btn-file">
                                                    Seleccionar… <input id="file" type="file" accept=".xls, .xlsx">
                                                </span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-sm-offset-3 col-sm-5">
                                        <button id="btnSubmit" type="button" class="btn btn-blue bt-submit">Subir Archivo</button>
                                    </div>
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
            <footer class="main">
                &copy; 2016 <strong>Administrador - Simulador Financiero</strong>
            </footer>
        </div>
    </div>
</div>
<input type="hidden" id="url" data-urlsubir="@Url.Action("Subir", "Administrador")" data-urllistado="@Url.Action("ListaArchivos", "Administrador")"/>
<script src="~/Scripts/SubirArchivo.js"></script>