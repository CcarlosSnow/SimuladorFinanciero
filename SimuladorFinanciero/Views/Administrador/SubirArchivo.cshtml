<div class="container">
    <div class="row">
        <div class="col-md-12">
            @using (Html.BeginForm("Subir", "Administrador", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                <h2>Cargar Archivo Base</h2>
                <br />
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-primary" data-collapsed="0">
                            <div class="panel-heading">
                                <div class="panel-title">
                                    Subir Excel
                                </div>
                                <div class="panel-options">
                                    <a href="#" data-rel="collapse"><i class="entypo-down-open"></i></a>
                                </div>
                            </div>
                            <div class="panel-body">
                                <form role="form" class="form-horizontal form-groups-bordered">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Archivo Excel</label>
                                        <div class="col-sm-5">
                                            <input type="file" class="form-control file2 inline btn btn-primary" data-label="<i class='fa fa-file-excel-o' aria-hidden='true'></i> Buscar" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <div class="col-sm-offset-3 col-sm-5">
                                            <button type="submit" class="btn btn-blue">Subir Archivo</button>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <footer class="main">
                    &copy; 2016 <strong>Administrador - Simulador Financiero</strong>
                </footer>
            }
        </div>

    </div>
</div>